<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_columnWeight="1"
    app:layout_gravity="center"
    app:layout_rowWeight="1" >

    <ImageView
        android:id="@+id/homeAlbum_goalAlbumImageView1"
        android:layout_width="156dp"
        android:layout_height="156dp"
        android:layout_gravity="center"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/homeAlbum_textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:includeFontPadding="false"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/noto_sans"
        android:lineSpacingExtra="49.03px"
        android:text="알고리즘 마스터하기"
        android:textFontWeight="600"
        android:textAlignment="textStart"
        android:textColor="@color/Black"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/homeAlbum_timeTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:includeFontPadding="false"
        android:layout_gravity="bottom"
        android:layout_marginLeft="39dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/noto_sans"
        android:lineSpacingExtra="81.72px"
        android:text="0 : 00 : 00"
        android:textFontWeight="500"
        android:textColor="@color/Black"
        android:textSize="20dp" />
</FrameLayout>