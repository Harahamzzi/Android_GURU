<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/pop_up_bg">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="270dp"
        android:layout_height="351dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/pop_lockCancelImageButton2"
            android:layout_width="90dp"
            android:layout_height="27dp"
            android:layout_marginStart="36dp"
            android:layout_marginBottom="27dp"
            android:background="#FFFFFF"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/cancel_button" />

        <ImageButton
            android:id="@+id/pop_settingOkImageButton2"
            android:layout_width="90dp"
            android:layout_height="27dp"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="27dp"
            android:background="@color/White"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/ok_button" />

        <ImageView
            android:id="@+id/pop_lockSetConfirmGoalColorImageView2"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="22dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/color_icon" />

        <TextView
            android:id="@+id/pop_goalTitleTextView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="20dp"
            android:drawablePadding="10dp"
            android:ems="8"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:maxLines="1"
            android:singleLine="true"
            android:text="목표 제목"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/pop_lockSetConfirmGoalColorImageView2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/pop_ImageView15"
            android:layout_width="237dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="56dp"
            android:background="@color/Gray"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/dot_line_horizontal" />

        <ImageView
            android:id="@+id/pop_ImageView16"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="71dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/timer_icon" />

        <TextView
            android:id="@+id/pop_TextView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="66dp"
            android:fontFamily="@font/noto_sans"
            android:text="잠금 시간"
            android:textColor="@color/Black"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/pop_ImageView16"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="99dp"
            android:layout_marginEnd="30dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/pop_hourTimeTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="bottom|center"
                android:inputType="textPersonName"
                android:typeface="monospace"
                android:fontFamily="@font/noto_sans"
                android:text="00"
                android:textColor="@color/Black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="bottom|center"
                android:text=":"
                android:textColor="@color/Black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/pop_minTmeTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="bottom|center"
                android:inputType="textPersonName"
                android:fontFamily="@font/noto_sans"
                android:text="1"
                android:textColor="@color/Black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="bottom|center"
                android:text=":"
                android:textColor="@color/Black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/pop_secTimeTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="bottom|center"
                android:inputType="textPersonName"
                android:fontFamily="@font/noto_sans"
                android:text="00"
                android:textColor="@color/Black"
                android:textSize="20sp"
                android:textStyle="bold" />

        </LinearLayout>

        <ImageView
            android:id="@+id/pop_imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="17dp"
            android:layout_marginTop="168dp"
            android:layout_weight="1"
            android:background="@drawable/ic_outline_app_registration_24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_outline_app_registration_24" />

        <TextView
            android:id="@+id/pop_textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="166dp"
            android:text="달성 가능한 세부 목표"
            android:fontFamily="@font/noto_sans"
            android:textColor="@color/Black"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/pop_imageView17"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/pop_ImageView17"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="195dp"
            android:layout_marginEnd="17dp"
            android:layout_marginBottom="10dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/pop_lockCancelImageButton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/hamster_inventory_bg"
            app:tint="@color/LightGray" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/pop_lockSettingConfirmDetailGoalRecyclerView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:padding="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            app:layout_constraintBottom_toBottomOf="@+id/pop_ImageView17"
            app:layout_constraintEnd_toEndOf="@+id/pop_ImageView17"
            app:layout_constraintStart_toStartOf="@+id/pop_ImageView17"
            app:layout_constraintTop_toTopOf="@+id/pop_ImageView17" />

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>