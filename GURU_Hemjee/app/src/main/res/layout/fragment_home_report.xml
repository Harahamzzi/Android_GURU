                android:layout_width="40dp"
                android:layout_height="20dp"
                android:layout_marginTop="67dp"
                android:background="@drawable/outline_round_button"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="주간"
                android:textAlignment="center"
                android:textFontWeight="300"
                android:textSize="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dailyGoalListLayout" />

            <TextView
                android:id="@+id/periodDayTextView"
                android:layout_width="98.3dp"
                android:layout_height="15dp"
                android:layout_marginTop="7dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="1월 1일 - 1월 7일"
                android:textAlignment="center"
                android:textFontWeight="400"
                android:textSize="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/weekTextView" />

            <TextView
                android:id="@+id/weekTimeTextView"
                android:layout_width="100dp"
                android:layout_height="29dp"
                android:layout_marginTop="1dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="0시간 00분"
                android:textAlignment="center"
                android:textFontWeight="700"
                android:textSize="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/periodDayTextView" />

            <com.github.mikephil.charting.charts.BarChart
