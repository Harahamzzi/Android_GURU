<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeReportFragment">

    <ScrollView
        android:id="@+id/homeReportScrollView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <!-- ***** 일간 ***** -->
            <TextView
                android:id="@+id/dailyTextView"
                android:layout_width="40dp"
                android:layout_height="20dp"
                android:background="@drawable/outline_round_button"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="일간"
                android:textAlignment="center"
                android:textFontWeight="300"
                android:textSize="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/dayTextView"
                android:layout_width="98.3dp"
                android:layout_height="15dp"
                android:layout_marginTop="7dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="1월 1일 월요일"
                android:textAlignment="center"
                android:textFontWeight="400"
                android:textSize="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dailyTextView" />

            <TextView
                android:id="@+id/todayTimeTextView"
                android:layout_width="100dp"
                android:layout_height="29dp"
                android:layout_marginTop="1dp"
                android:fontFamily="@font/noto_sans"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="0시간 00분"
                android:textAlignment="center"
                android:textFontWeight="700"
                android:textSize="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dayTextView" />

            <ProgressBar
                android:id="@+id/totalProgressBar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="327dp"
                android:layout_height="16dp"
                android:layout_marginTop="17dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/todayTimeTextView" />

            <!-- 일간 목표들 목록.. -->
            <LinearLayout
                android:id="@+id/dailyGoalListLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="17dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/totalProgressBar">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="32dp">

                    <ImageView
                        android:id="@+id/dayGoalColorImageView1"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_marginStart="17dp"
                        android:src="@drawable/ic_colorselectionicon"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/dayGoalTextView1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="50dp"
                        android:fontFamily="@font/noto_sans"
                        android:includeFontPadding="false"
                        android:singleLine="true"
                        android:text="알고리즘 마스터하기"
                        android:textAlignment="textStart"
                        android:textFontWeight="400"
                        android:textSize="12dp"
                        app:layout_constraintEnd_toStartOf="@+id/dayTimeTextView1"
                        app:layout_constraintStart_toEndOf="@+id/dayGoalColorImageView1"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/dayTimeTextView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="17dp"
                        android:fontFamily="@font/noto_sans"
                        android:includeFontPadding="false"
                        android:text="0시간 00분"
                        android:textAlignment="textEnd"
                        android:textFontWeight="700"
                        android:textSize="12dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ProgressBar
                        android:id="@+id/dayProgressBar1"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="327dp"
                        android:layout_height="12dp"
                        android:layout_marginStart="17dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginEnd="17dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/dayGoalTextView1" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.legacy.widget.Space
                    android:layout_width="match_parent"
                    android:layout_height="17dp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="32dp">

                    <ImageView
                        android:id="@+id/dayGoalColorImageView2"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_marginStart="17dp"
                        android:src="@drawable/ic_colorselectionicon"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/dayGoalTextView2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="50dp"
                        android:fontFamily="@font/noto_sans"
                        android:includeFontPadding="false"
                        android:singleLine="true"
                        android:text="알고리즘 마스터하기"
                        android:textAlignment="textStart"
                        android:textFontWeight="400"
                        android:textSize="12dp"
                        app:layout_constraintEnd_toStartOf="@+id/dayTimeTextView2"
                        app:layout_constraintStart_toEndOf="@+id/dayGoalColorImageView2"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/dayTimeTextView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="17dp"
                        android:fontFamily="@font/noto_sans"
                        android:includeFontPadding="false"
                        android:text="0시간 00분"
                        android:textAlignment="textEnd"
                        android:textFontWeight="700"
                        android:textSize="12dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ProgressBar
                        android:id="@+id/dayProgressBar2"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="327dp"
                        android:layout_height="12dp"
                        android:layout_marginStart="17dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginEnd="17dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/dayGoalTextView2" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>


            <!-- ***** 주간 ***** -->
            <TextView
