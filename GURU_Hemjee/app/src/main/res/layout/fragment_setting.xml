<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".SettingFragment">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@color/BrightYellow"
        android:fontFamily="@font/noto_sans"
        android:paddingLeft="17dp"
        android:text="계정"
        android:textColor="@color/Black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:paddingLeft="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:background="@drawable/ic_outline_badge_24"
            android:src="@drawable/ic_outline_badge_24" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="#00F4D1B5"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:paddingLeft="17dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="닉네임 변경"
            android:textColor="@color/Black"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_outline_language_24"
            android:background="@drawable/ic_outline_language_24"
            app:srcCompat="@drawable/ic_outline_language_24" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="#00F4D1B5"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:paddingLeft="17dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="계정 수정"
            android:textColor="@color/Black"
            android:textSize="18sp" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/BrightYellow"
        android:fontFamily="@font/noto_sans"
        android:paddingLeft="17dp"
        android:text="고객 지원"
        android:textColor="@color/Black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:paddingLeft="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_outline_support_agent_24"
            android:background="@drawable/ic_outline_support_agent_24"
            app:srcCompat="@drawable/ic_outline_support_agent_24" />

        <Button
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="#00F4D1B5"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:paddingLeft="17dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="문의하기"
            android:textColor="@color/Black"
            android:textSize="18sp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:paddingLeft="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_outline_business_24"
            android:background="@drawable/ic_outline_business_24"
            app:srcCompat="@drawable/ic_outline_business_24" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="#00F4D1B5"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:paddingLeft="17dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="회사 소개하기"
            android:textColor="@color/Black"
            android:textSize="18sp" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/BrightYellow"
        android:fontFamily="@font/noto_sans"
        android:paddingLeft="17dp"
        android:text="기타"
        android:textColor="@color/Black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:paddingLeft="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_outline_no_accounts_24"
            android:background="@drawable/ic_outline_no_accounts_24"
            app:srcCompat="@drawable/ic_outline_no_accounts_24" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="#00F4D1B5"
            android:fontFamily="@font/noto_sans"
            android:gravity="center|left"
            android:paddingLeft="17dp"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="회원 탈퇴하기"
            android:textColor="@color/Black"
            android:textSize="18sp" />
    </LinearLayout>
</LinearLayout>